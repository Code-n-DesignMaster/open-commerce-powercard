{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {
  },
  "rules": {
    "no-console": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "space-in-parens": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      160
    ],
    "ordered-imports": [
      false,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ],
    "no-shadowed-variable": false,
    "no-import-side-effect": [
      true,
      "db"
    ],
    "no-magic-numbers": [
      false,
      -1,
      0,
      1,
      200,
      201,
      401,
      403
    ],
    "no-unnecessary-type-assertion": true,
    "no-floating-promises": true,
    "cyclomatic-complexity": [
      true,
      20
    ],
    "no-multi-spaces": [
      "error",
      {
        "exceptions": {
          "Property": true
        }
      }
    ],
    "indent": [
      true,
      2
    ],
    "key-spacing": [
      "error",
      {
        "align": "value"
      }
    ],
    "object-curly-spacing": [
      true,
      "always"
    ]
  },
  "rulesDirectory": []
}